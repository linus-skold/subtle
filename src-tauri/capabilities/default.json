{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions",
  "windows": [
    "main"
  ],
  "permissions": [
    "core:default",
    "opener:default",
    "sql:default",
    "sql:allow-execute",
    "core:window:default",
    "core:window:allow-start-dragging",
    "core:window:allow-set-size",
    "fs:default",
    "fs:read-all",
    "fs:write-all",
    "core:window:allow-close"
  ]
}